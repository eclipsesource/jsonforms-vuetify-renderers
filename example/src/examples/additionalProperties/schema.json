{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "propertiesString": {
      "type": "string"
    }
  },
  "propertyNames": {
    "minLength": 2
  },
  "patternProperties": {
    "^string$": {
      "type": "string"
    },
    "^number$": {
      "type": "number"
    },
    "^integer$": {
      "type": "integer"
    },
    "^object$": {
      "type": "object",
      "properties": {
        "prop1": {
          "type": "string"
        }
      }
    },
    "^boolean$": {
      "type": "boolean"
    },
    "^stringArray$": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "^numberArray$": {
      "type": "array",
      "items": {
        "type": "number"
      }
    },
    "^integerArray$": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    },
    "^objectArray$": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "prop1": {
            "type": "string"
          }
        }
      }
    },
    "^booleanArray$": {
      "type": "array",
      "items": {
        "type": "boolean"
      }
    }
  },
  "additionalProperties": {
    "type": "string",
    "title": "Additional Properties"
  },
  "maxProperties": 15
}
