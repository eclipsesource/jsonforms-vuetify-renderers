{
  "name": "@jsonforms/vue2-vuetify-webcomponent",
  "version": "3.0.0-rc.1",
  "description": "Vue 2 Vuetify renderers for JSON Forms implements as webcomponent",
  "repository": "https://github.com/eclipsesource/jsonforms-vuetify-renderers",
  "bugs": "https://github.com/eclipsesource/jsonforms/issues",
  "homepage": "http://jsonforms.io/",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vue",
    "vue2",
    "vue 2",
    "vuetify",
    "form",
    "forms",
    "json",
    "jsonforms",
    "frontend",
    "generator",
    "input",
    "renderengine",
    "jsonschema",
    "schema",
    "uischema",
    "layout",
    "customization",
    "webcomponent"
  ],
  "main": "lib/jsonforms-vue2-vuetify-webcomponent.cjs.js",
  "module": "lib/jsonforms-vue2-vuetify-webcomponent.esm.js",
  "typings": "lib/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prebuild": "rimraf dist && npm run wc-use-contenthash",
    "wc-use-contenthash": "node ./patch-resolveWcEntry.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target wc-async --inline-vue 'src/web-components/*.vue'"
  },
  "dependencies": {
    "@jsonforms/core": "3.0.0-rc.1",
    "@jsonforms/vue2": "3.0.0-rc.1",
    "@jsonforms/vue2-vuetify": "3.0.0-rc.1",
    "@mdi/font": "^7.0.96",
    "ajv-keywords": "^5.1.0",
    "core-js": "^3.9.1",
    "json-refs": "^3.0.15",
    "vue": "^2.7.0",
    "vue-plugin-load-script": "^1.3.6",
    "vuetify": "^2.4.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.18",
    "@vue/cli-plugin-eslint": "~4.5.18",
    "@vue/cli-plugin-typescript": "~4.5.18",
    "@vue/cli-service": "~4.5.18",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "typescript": "^4.7.3",
    "vue-cli-plugin-vuetify": "~2.4.2",
    "vuetify-loader": "^1.7.0",
    "replace-in-file": "^6.3.5"
  }
}
